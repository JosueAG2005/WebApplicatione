@model WebApplication9.Models.Inscripcione

@{
    ViewData["Title"] = "Detalles de Inscripción";
}

<div class="container mt-5" style="padding-left:50px;">
    <h1 class="text-primary">Detalles de Inscripción</h1>

    <div class="card p-4">
        <h4 class="card-title">Inscripción</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.FechaInscripcion)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.FechaInscripcion)
            </dd>
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.IdConcursoNavigation.Nombre)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.IdConcursoNavigation.Nombre)
            </dd>
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation.Nombre)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.IdUsuarioNavigation.Nombre)
            </dd>
        </dl>
    </div>

    <div class="mt-4">
        <a asp-action="Edit" asp-route-id="@Model?.IdInscripcion" class="btn btn-warning">Editar</a>
        <a asp-action="Index" class="btn btn-secondary">Volver a la Lista</a>
    </div>
</div>
